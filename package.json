    "@prisma/client": "^5.18.0",
    "@types/bcrypt": "^5.0.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "csv-parser": "^3.0.0",
    "geolib": "^3.3.4",
    "git": "^0.1.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
